{
	"$schema": "https://json.schemastore.org/block.json",
	"apiVersion": 2,
	"name": "fun-slides/fun-slides",
	"version": "0.1.0",
	"title": "Fun Slides",
	"category": "media",
	"icon": "smiley",
	"description": "Fun Slides for Gutenberg!",
	"supports": {
		"html": false
	},
	"textdomain": "fun-slides",
	"editorScript": "file:./build/index.js",
	"editorStyle": "file:./build/index.css",
	"style": "file:./build/style-index.css",
	"attributes": {
		"totalSlides": {
			"type": "integer"
		},
		"tempTotalSlides": {
			"type": "integer",
			"default": 3
		},
		"slideshow": {
			"type": "array",
			"default": []
		},
		"slideTimer": {
			"type": "integer",
			"default": 3000
		},
		"showSliderDots": {
			"type": "boolean",
			"default": true
		},
		"showNavArrows": {
			"type": "boolean",
			"default": true
		},
		"autoplay": {
			"type": "boolean",
			"default": true
		}
	}
}